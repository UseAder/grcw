/* 验证弹框 */

.attr-pop {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 4;
}

.popup-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  height: 100%;
  z-index: 5;
}

.popup-contents {
  position: fixed;
  margin: 0 auto;
  width: 311px;
  height: 283px;
  background-color: #fff;
  z-index: 6;
  /* overflow-x: hidden; */
  top:20%;
  left: 0px;
  right: 0px;
  /* margin-top: 80px; */
  bottom: 0px;
  /* overflow-y: hidden; */
}

.popup-contents .login {
  display: flex;
  flex-direction: column;
}

.popup-contents .login .login_logo {
  width: 57px;
  height: 42px;
  margin: 0 auto;
  margin-top: 18px;
}

.login_account {
  margin: 0 auto;
  width: 271px;
  height: 41px;
  border-top: 1px solid #b5b5b5;
  border-left: 1px solid #b5b5b5;
  border-right: 1px solid #b5b5b5;
  border-bottom: 1px solid #b5b5b5;
  line-height: 41px;
  display: flex;
  margin-top: 23px;
  flex-direction: row;
  align-items: center;
}

.pwd-content {
  width: 271px;
  margin: 0 auto;
  justify-content: space-between;
}

.pwd_account {
  position: relative;
  width: 155px;
  padding-right: 5px;
  height: 41px;
  border-bottom: 1px solid #b5b5b5;
  border-left: 1px solid #b5b5b5;
  border-right: 1px solid #b5b5b5;
  border-top: 1px solid #b5b5b5;
  line-height: 41px;
  display: flex;
  /* margin-top: 15px; */
  flex-direction: row;
  align-items: center;
}

.pwd-content .page-vcode-btn::after {
  border: none;
}

.pwd-content .page-vcode-btn {
  background: #fff;
  color: #000;
  /* margin-top: 15px; */
  font-size: 28rpx;
  height: 42px;
  line-height: 42px;
  border-bottom: 1px solid #b5b5b5;
  border-left: 1px solid #b5b5b5;
  border-right: 1px solid #b5b5b5;
  border-top: 1px solid #b5b5b5;
  border-radius: 0;
  margin-left: 8px;
  width: 132px;
}

.login_account input {
  margin-left: 10px;
  color: #333;width:100%;

}

.pwd_account input {
  margin-left: 10px;
  color: #333;
}

.account_logo {
  width: 18px;
  margin-left: 5px;
}

.bt {
  width: 186px;
  height: 39px;
  background: #f90;
  margin: 0 auto;
  margin-top: 40px;
  color: white;
  font-size: 14px;
  text-align: center;
  align-content: center;
  position: relative;
  line-height: 39px;
  border-radius: 3px;
  overflow: hidden;
}

.close_container {
  position: absolute;
  width: 100%;
margin-top:125%;
bottom:-85rpx;

  align-content: center;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.close {
  width: 33px;
  height: 33px;
  left: 0px;
  top: 0px;
  right: 0px;
  bottom: 0px;
  z-index: 999;
  position: relative;
}

.pwd_account input {
  width: 130px;
}

page::after{
    content: '';
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 2rpx;
    background-color: rgba(0, 0, 0, 0.07);
    z-index: 9999;
}
